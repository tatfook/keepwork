{
  "presets": [
    ["env", {
      "modules": false
    }],
    "stage-2"
  ],
  "plugins": [
    "transform-object-rest-spread",
    "syntax-dynamic-import",
    "transform-object-assign",
    "transform-vue-jsx",
    "transform-runtime",
    "dynamic-import-node"
  ],
  "ignore": [
    "app/web/lib/mod/parser/*",
    "app/web/lib/global.js"
  ],
  "comments": false,
  "env": {
    "test": {
      "presets": ["env", "stage-2"],
      "plugins": ["transform-vue-jsx", "istanbul"]
    }
  }
}
